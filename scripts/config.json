            ]
        },
        "CodePointOpen": {
            "name": "Code Point Open",
            "formats": [
                "CSV"
            ],
            "script":"postcodes/",
            "targets": [
                {
                    "directory":"postcodes",
                    "datasets":[
                        {
                            "targets":["EX1.json"],
                            "source":"Data/CSV/",
                            "sourcefiles":["ex.csv","pl.csv","tq.csv"],
                            "script":"postcode.sh"
                        }
                    ]
                }
            ]
        },
        "OpenUPRN":{
            "name": "Open UPRN",
            "formats": [
                "CSV"
            ],
            "clip":"/boundaries/Teignbridge.json",
            "script":"uprn/",
            "targets": [
                {
                    "directory":"uprn",
                    "datasets":[
                        {
                            "targets":["teignbridge_uprn.csv"],
                            "source":"osopenuprn_{VERSION}.csv",
                            "sourcefiles":["uprn.csv"],
                            "script":"uprn.sh"
                        }
                    ]
                }
